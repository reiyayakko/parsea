{
  "name": "parsea",
  "version": "0.9.0",
  "description": "parser combinator library for parsing ArrayLike.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./internal": {
      "require": "./dist/internal.cjs",
      "import": "./dist/internal.js",
      "types": "./dist/internal.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "biome format .",
    "lint": "biome lint .",
    "check": "biome check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uzmoi/parsea.git"
  },
  "keywords": [
    "parsing"
  ],
  "author": "uzmoi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uzmoi/parsea/issues"
  },
  "homepage": "https://github.com/uzmoi/parsea#readme",
  "dependencies": {
    "emnorst": "^1.0.0-next.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.1",
    "@swc/jest": "^0.2.29",
    "jest": "^29.7.0",
    "lefthook": "^1.5.6",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
