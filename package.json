{
  "name": "parsea",
  "version": "0.6.0",
  "description": "parser combinator library for parsing ArrayLike.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./internal": {
      "require": "./dist/internal.cjs",
      "import": "./dist/internal.js",
      "types": "./dist/internal.d.ts"
    }
  },
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "test": "jest",
    "fmt:check": "prettier **/*.ts -c",
    "fmt": "prettier **/*.ts -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rizzzse/parsea.git"
  },
  "keywords": [
    "parsing"
  ],
  "author": "rizzzse <rizzzse@gmail.com> (https://github.com/rizzzse)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rizzzse/parsea/issues"
  },
  "homepage": "https://github.com/rizzzse/parsea#readme",
  "dependencies": {
    "emnorst": "^1.0.0-next.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "esbuild": "^0.17.4",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.4.0",
    "prettier": "^2.8.3",
    "tsup": "^6.7.0",
    "typescript": "^4.9.4"
  }
}
