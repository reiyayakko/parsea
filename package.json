{
  "name": "parsea",
  "version": "0.10.0",
  "description": "parser combinator library for parsing ArrayLike.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./internal": {
      "require": "./dist/internal.cjs",
      "import": "./dist/internal.js",
      "types": "./dist/internal.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "check": "biome check .",
    "spell": "cspell --no-progress ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uzmoi/parsea.git"
  },
  "keywords": [
    "parsing"
  ],
  "author": "uzmoi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uzmoi/parsea/issues"
  },
  "homepage": "https://github.com/uzmoi/parsea#readme",
  "dependencies": {
    "emnorst": "^1.0.0-next.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@vitest/coverage-v8": "^2.1.1",
    "cspell": "^8.14.4",
    "lefthook": "^1.7.15",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
