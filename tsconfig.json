{
  "include": ["**/*", ".*"],
  "exclude": ["dist/*"],
  "compilerOptions": {
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "ES2020",
    "target": "ESNext",
    "lib": ["ESNext"],
    "strict": true,
    "noEmit": true,
    "stripInternal": true,
    "paths": { "parsea": ["./src"] },
  },
}
